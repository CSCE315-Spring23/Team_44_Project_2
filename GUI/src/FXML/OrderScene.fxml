<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox prefHeight="400.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <TilePane prefHeight="150.0" prefWidth="410.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </TilePane>
            <TilePane prefHeight="150.0" prefWidth="410.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </TilePane>
            <TilePane prefHeight="100.0" prefWidth="410.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </TilePane>
         </children></VBox>
   </center>
   <left>
      <VBox fx:id="navBar" alignment="TOP_CENTER" prefHeight="400.0" prefWidth="103.0">
         <children>
            <Button fx:id="orderButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#navButtonClicked" text="Order">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
               </padding>
            </Button>
            <Button fx:id="orderHistoryButton" alignment="CENTER" contentDisplay="CENTER" layoutX="21.0" layoutY="20.0" mnemonicParsing="false" onAction="#navButtonClicked" text="Order History" textAlignment="CENTER" wrapText="true">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="15.0" top="15.0" />
               </padding>
            </Button>
            <Button fx:id="inventoryButton" alignment="CENTER" contentDisplay="CENTER" layoutX="19.0" layoutY="78.0" mnemonicParsing="false" onAction="#navButtonClicked" text="Inventory" textAlignment="CENTER" wrapText="true">
               <padding>
                  <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
               </padding>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="employeesButton" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="136.0" mnemonicParsing="false" onAction="#navButtonClicked" text="Employees" textAlignment="CENTER" wrapText="true">
               <padding>
                  <Insets bottom="15.0" left="8.0" right="8.0" top="15.0" />
               </padding>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="editMenuButton" alignment="CENTER" contentDisplay="CENTER" layoutX="19.0" layoutY="194.0" mnemonicParsing="false" onAction="#navButtonClicked" text="Edit Menu" textAlignment="CENTER" wrapText="true">
               <padding>
                  <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
               </padding>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button fx:id="logoutButton" mnemonicParsing="false" text="Logout">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
   <right>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Current Order" />
            <Label fx:id="orderListLabel" text="Label" />
            <Label text="Customer Name" />
            <TextField fx:id="customerNameField" />
            <HBox alignment="CENTER" prefHeight="30.0" prefWidth="100.0">
               <children>
                  <Label text="\$" />
                  <Label fx:id="totalCostLabel" text="Label" />
               </children>
            </HBox>
            <Button fx:id="submitOrderButton" mnemonicParsing="false" text="Order" />
         </children>
      </VBox>
   </right>
</BorderPane>
